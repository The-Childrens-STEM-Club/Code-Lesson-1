from pip._vendor.distlib.compat import raw_input

#Comments
#You can write a single line comment with #
'''
You can write a multiple line comment with '''

#Variables
a=1
b=1
print(a+b)

#You can also print words or 'Strings"

J='Jon'
print("Hello", J)

#You can also get input
q = raw_input('What is your name?')
print('Hello', q)

#If Statement
print(a,b) if a == b else print(a, 'is not equal to', b)

#for loops
for x in range(5):
    print('Hi', J)
    
#While Loop
i = 1
while i< 6:
    print(i)
    i += 1

